->Temperature

==Temperature_Change
~die01 = RANDOM(1,6)
~die02 = RANDOM(1,6)

{
    -die01+die02 == 2:
        ~code -= 3
        ~prech = true
    -die01+die02 == 3:
        ~code -= 2
        ~prech = false
    -die01+die02 == 4:
        ~code -= 2
        ~prech = true
    -die01+die02 == 5:
        ~code -= 1
        ~prech = false
    -die01+die02 == 6:
        ~code -= 1
        ~prech = true
    -die01+die02 == 8:
        ~code += 1
        ~prech = true
    -die01+die02 == 9:
        ~code += 1
        ~prech = false
    -die01+die02 == 10:
        ~code += 2
        ~prech = true
    -die01+die02 == 11:
        ~code += 2
        ~prech = false
    -die01+die02 == 12:
        ~code += 3
        ~prech = true
}
->Precipitation

==Precipitation
{
    -prech:
    {
        -season == "winterborn" || season == "deepwinter" || season == "winterfade":
            {
                -region == "cradle waste desert":
                    -die01 > die02:
                        ~pre = "None"
                    -die01 == die02:
                        ~pre = "Trace"
                    -die01 < die02:
                        ~pre = "Light (1 inch snow)"
                -region == "crag claw desert":
                    -die01 > die02:
                        ~pre = "None"
                    -die01 == die02:
                        ~pre = "Trace"
                    -die01 < die02:
                        ~pre = "Light (1 inch snow)"
                -region == "woods":
                    -die01 > die02:
                        ~pre = "Trace"
                    -die01 == die02:
                        ~pre = "Moderate (2 inches snow)"
                    -die01 < die02:
