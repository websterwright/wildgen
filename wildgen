->Encounter

==Encounter
{
    -encounter == true:
        {
            -region == "cradle waste desert":
                ->CWD_Encounter
            -region == "crag claw desert":
                ->CCD_Encounter
            -region == "woods":
                ->Woods_Encounter
            -region == "cradle waste mountains":
                ->CWM_Encounter
            -region == "crag claw mountains":
                ->CCM_Encounter
            -region == "crooked crone mountains":
                ->Crone_Encounter
            -region == "little foot mountains":
                ->Little_Encounter
            -region == "rockwall mountains":
                ->Rockwall_Encounter
            -region == "stonewall mountains":
                ->Stoneslant_Encounter
            -region == "three sisters mountains":
                ->3Sis_Encounter
            -region == "beldam's bog swamp":
                ->Beldam_Encounter
            -region == "whispering wood swamp":
                ->Whisper_Encounter
            -region == "plains":
                ->Plains_Encounter
            -region == "river":
                ->River_Encounter
        }
    -else:
        ->Result
}

